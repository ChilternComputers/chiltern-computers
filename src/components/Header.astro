    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const header = document.getElementById('main-header');
            const headerContent = document.getElementById('header-content');
            const scrollProgress = document.getElementById('scroll-progress');
            const trustBanner = document.getElementById('trust-banner');
            const mobileMenuToggle = document.getElementById('mobileMenuToggle');
            const mobileMenu = document.getElementById('mobileMenu');
            const mobileMenuOverlay = document.getElementById('mobileMenuOverlay');
            const closeMobileMenu = document.getElementById('closeMobileMenu');
            const menuLines = document.querySelectorAll('.menu-line');
            
            let isMobileMenuOpen = false;
            let lastScrollY = 0;
            let ticking = false;

            // Enhanced scroll effects with translucent header
            function updateHeaderOnScroll() {
                const scrollY = window.scrollY;
                const docHeight = document.documentElement.scrollHeight - window.innerHeight;
                const scrollPercent = Math.min((scrollY / docHeight) * 100, 100);

                // Update scroll progress bar
                if (scrollProgress) {
                    scrollProgress.style.width = `${scrollPercent}%`;
                }

                // Calculate translucency based on scroll position
                const maxScroll = 300; // Adjust this value to control when full translucency is reached
                const scrollRatio = Math.min(scrollY / maxScroll, 1);
                
                // Header styling changes with translucent effect
                if (scrollY > 10) {
                    // Progressive translucency: starts at 98% opacity, goes down to 25% when fully scrolled
                    const opacity = 0.98 - (scrollRatio * 0.73); // 0.98 to 0.25
                    const blurAmount = 12 + (scrollRatio * 8); // 12px to 20px blur
                    
                    header.style.backgroundColor = `rgba(255, 255, 255, ${opacity})`;
                    header.style.backdropFilter = `blur(${blurAmount}px)`;
                    header.style.WebkitBackdropFilter = `blur(${blurAmount}px)`;
                    header?.classList.add('shadow-lg');
                    header?.classList.remove('shadow-md');
                    
                    if (headerContent) {
                        headerContent.style.height = '3rem';
                    }
                    
                    // Hide trust banner on scroll with smooth transition
                    if (trustBanner) {
                        trustBanner.style.transform = 'translateY(-100%)';
                        trustBanner.style.opacity = '0';
                        trustBanner.style.visibility = 'hidden';
                    }
                    
                    // Move header to top when banner is hidden
                    header.style.top = '0';
                } else {
                    // Reset to normal state
                    header.style.backgroundColor = 'rgba(255, 255, 255, 0.95)';
                    header.style.backdropFilter = 'blur(12px)';
                    header.style.WebkitBackdropFilter = 'blur(12px)';
                    header?.classList.add('shadow-md');
                    header?.classList.remove('shadow-lg');
                    
                    if (headerContent) {
                        headerContent.style.height = '4rem';
                    }
                    
                    // Show trust banner
                    if (trustBanner) {
                        trustBanner.style.transform = 'translateY(0)';
                        trustBanner.style.opacity = '1';
                        trustBanner.style.visibility = 'visible';
                    }
                    
                    // Position header below banner when banner is visible
                    header.style.top = '2rem';
                }

                // Smart header hide/show (optional - you can remove this if you want header always visible)
                if (scrollY > 100) {
                    if (scrollY > lastScrollY && scrollY > 200 && !isMobileMenuOpen) {
                        // Scrolling down - hide header
                        header.style.transform = 'translateY(-100%)';
                    } else {
                        // Scrolling up - show header
                        header.style.transform = 'translateY(0)';
                    }
                }
                
                lastScrollY = scrollY;
                ticking = false;
            }

            function requestScrollUpdate() {
                if (!ticking) {
                    requestAnimationFrame(updateHeaderOnScroll);
                    ticking = true;
                }
            }

            window.addEventListener('scroll', requestScrollUpdate, { passive: true });

            // Mobile menu functionality
            function openMobileMenu() {
                isMobileMenuOpen = true;
                document.body.style.overflow = 'hidden';
                mobileMenuOverlay?.classList.remove('hidden');
                mobileMenu?.classList.remove('translate-x-full');
                
                // Animate hamburger to X
                menuLines.forEach((line, index) => {
                    if (index === 0) line.style.transform = 'rotate(45deg) translateY(6px)';
                    if (index === 1) line.style.opacity = '0';
                    if (index === 2) line.style.transform = 'rotate(-45deg) translateY(-6px)';
                });
                
                setTimeout(() => {
                    mobileMenuOverlay?.classList.remove('opacity-0');
                }, 10);
            }

            function closeMobileMenuFunc() {
                isMobileMenuOpen = false;
                document.body.style.overflow = '';
                mobileMenuOverlay?.classList.add('opacity-0');
                mobileMenu?.classList.add('translate-x-full');
                
                // Reset hamburger
                menuLines.forEach(line => {
                    line.style.transform = '';
                    line.style.opacity = '';
                });
                
                setTimeout(() => {
                    mobileMenuOverlay?.classList.add('hidden');
                }, 300);
            }

            // Event listeners
            mobileMenuToggle?.addEventListener('click', openMobileMenu);
            closeMobileMenu?.addEventListener('click', closeMobileMenuFunc);
            mobileMenuOverlay?.addEventListener('click', closeMobileMenuFunc);

            // Keyboard navigation
            document.addEventListener('keydown', (e) => {
                if (e.key === 'Escape' && isMobileMenuOpen) {
                    closeMobileMenuFunc();
                }
            });

            // Add staggered animation to nav items
            const navLinks = document.querySelectorAll('.nav-link');
            navLinks.forEach((link, index) => {
                link.style.animationDelay = `${index * 100}ms`;
            });
        });
    </script>
