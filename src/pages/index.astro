    @import url('https://fonts.googleapis.com/css2?family=Orbitron:wght@400;600;700&family=Exo+2:wght@300;400;500;600&family=Space+Mono:wght@400;700&display=swap');
    
    :root {
      --cosmic-blue: #1e3a8a;
      --nebula-purple: #7c3aed;
