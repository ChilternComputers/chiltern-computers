    width: 20px;
    height: 20px;
    border-radius: 50%;
    background: linear-gradient(45deg, var(--aurora-green), var(--plasma-cyan));
    display: flex;
    align-items: center;
    justify-content: center;
  }

  /* Service Areas */
  .service-area-primary {
    color: var(--plasma-cyan);
    font-size: 12px;
    font-weight: 600;
    white-space: nowrap;
  }

  .service-area {
    color: var(--comet-silver);
    font-size: 12px;
    white-space: nowrap;
  }

  /* Ticker container and content styling - FIXED: Single content, CSS animation */
